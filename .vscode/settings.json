{
  "python.pythonPath": "venv/bin/python",
  "python.linting.pylintEnabled": true,
  "python.formatting.provider": "black",
  "cSpell.words": [
    "BINANCE",
    "OHLCV",
    "USDT",
    "nargs",
    "ohlcvs",
    "prec",
    "pytest",
    "pytz",
    "reorderd",
    "retrade",
    "tzinfo"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/venv/*/**": true
  }
}
